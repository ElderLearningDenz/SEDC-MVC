@using BurgerAppDemo.Models.ViewModels

@model List<OrderDetailsViewModel>

<h1>Order List:</h1>

<ul>
    @for (int i = 0; i < Model.Count; i++)
        {
            <li>Name: @Model.ElementAt(i).BurgerName -- User: @Model.ElementAt(i).UserName -- Location: @Model.ElementAt(i).Location -- Delivered: @Model.ElementAt(i).IsDelivered -- Price: @Model.ElementAt(i).Price</li>
        }
</ul>

